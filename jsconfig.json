{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "es2022",
    "allowImportingTsExtensions": true,
    "checkJs": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}